{
  "name": "vibe-button",
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && vite build --config vite-site.config.ts",
    "build:types": "tsc lib/index.ts --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist",
    "lib",
    "example.html",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "devDependencies": {
    "@types/node": "^22.15.30",
    "prettier": "^3.5.3",
    "shiki": "^3.6.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@rehype-pretty/transformers": "^0.13.2"
  }
}